<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.finance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Finance"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".Entrada">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login"/>
        <activity android:name=".register"/>
        <activity android:name=".principal"/>
        <activity android:name=".profile"/>
        <activity android:name=".report"/>
        <activity android:name=".AdicionarCategorias"
            android:theme="@style/Theme.AppCompat.Dialog"/>
        <activity android:name=".Lancamento"/>
        <activity android:name=".Filtro"
            android:theme="@style/Theme.AppCompat.Dialog"/>
        <activity android:name=".Fornecedor"
            android:theme="@style/Theme.AppCompat.Dialog"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>